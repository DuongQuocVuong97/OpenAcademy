<odoo>
    <data>
        <template id="index">
            <t t-call="website.layout">
            <t t-set="title">Session List</t>
            <div class="oe_structure">
                <div class="container">
                    <t t-foreach="session" t-as="session">
                        <p> <t t-esc="session.name"/> </p>
                    </t>
                </div>
            </div>
            </t>
        </template>
    </data>
</odoo>